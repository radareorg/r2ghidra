NAME=AARCH64:LE:64:v8A:gcc
FILE=malloc://2048
EXPECT=<<EOF
            0x00000000      298947f9       ldr x9, [x9, #0xf10]
type: load
----
            0x00000200      690200f9       str x9, [x19]
type: store
EOF
CMDS=<<EOF
e asm.cpu=AARCH64:LE:64:v8A:gcc
e asm.arch=r2ghidra
wx 298947f9
pd 1
ao | grep type
?e ----
wx 690200f9 @ 0x200
pd 1 @ 0x200
ao @ 0x200| grep type
EOF
RUN

