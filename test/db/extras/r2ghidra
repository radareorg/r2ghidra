NAME=comments
FILE=bins/elf/crackme0x05
EXPECT=<<EOF

uint main(void)

{
    uchar auStack_7c [120];
    
    //main starts here
    //printf call here
    sym.imp.printf("IOLI Crackme Level 0x05\n");
    sym.imp.printf("Password: ");
    //scanf call here
    sym.imp.scanf(0x80486b2,auStack_7c);
    sym.check(auStack_7c);
    //main returns here
    return 0;
}

